{
  "name": "nonempty-list",
  "version": "4.0.0",
  "description": "A list that always has at least one thing in it.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Ryan L. Bell <ryan.l.bell@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kofno/nonempty-list.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "docs": "typedoc --out docs && touch docs/.nojekyll",
    "test": "bun test tests/**/*.ts",
    "build": "tsup src/index.ts --format cjs,esm --dts --external maybeasy --external resulty",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external maybeasy --external resulty --watch"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "tsup": "^8.0.1",
    "typedoc": "^0.27.9",
    "typescript": "^5.8.2",
    "@types/bun": "^1.2.4"
  },
  "dependencies": {
    "maybeasy": "^6.3.0",
    "resulty": "^8.0.0"
  },
  "peerDependencies": {
    "maybeasy": "^6.3.0",
    "resulty": "^8.0.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100
  }
}
